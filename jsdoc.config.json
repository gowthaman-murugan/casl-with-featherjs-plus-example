{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include":["src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "encoding": "utf8",
         "destination": "./docs/",
        "recurse": true,
        "verbose": true
      }
}